{
  "name": "@xeedware/cognito-jwt",
  "version": "1.2.4",
  "description": "AWS Cognito AccessToken and IdToken classes. ",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "files": [
    "dist/"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p ./src",
    "test": "mocha -r ts-node/register test/specs/*.spec.ts",
    "testcov": "nyc mocha -r ts-node/register test/specs/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xeedware/cognito-jwt.git"
  },
  "keywords": [
    "AWS",
    "Cognito",
    "AccessToken",
    "IdToken",
    "jwt",
    "token verification",
    "verification"
  ],
  "author": "Al Tabayoyon <alt@xeedware.org>",
  "license": "MIT",
  "homepage": "https://github.com/xeedware/cognito-jwt#readme",
  "dependencies": {
    "@types/jsonwebtoken": "^8.5.0",
    "jsonwebtoken": "^9.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.6.0",
    "aws-sdk": "^2.735.0",
    "chai": "^4.2.0",
    "jwk-to-pem": "^2.0.4",
    "mocha": "^8.1.1",
    "nyc": "^15.1.0",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "mocha": {
    "timeout": 15000
  }
}
